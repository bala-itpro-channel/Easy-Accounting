{
  "name": "accounts-project",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cc": "jest --coverage=true src/app",
    "lint": "npm-run-all scsslint tslint",
    "tslint": "tslint -c tslint.json 'src/**/*.ts' -t stylish --fix",
    "scsslint": "sass-lint -v",
    "e2e": "ng e2e",
    "start": "check-engines && npm-run-all stop start:mock start:dev",
    "start:dev": "ng serve --port=4200 --host=easy-accounting.webfrontier.ca --no-live-reload",
    "start:mock": "mock-responses --db-path=./mock-server/mock-responses.sql --port=9300  --ssl --cookie='PLAY_SESSION=ACCTNBR=123456789; Path=/' --headers='Access-Control-Allow-Headers=Content-Type, Access-Control-Allow-Headers, Authorization, X-Requested-With, Ocp-Apim-Subscription-Key, appsource, requestId, flow' &",
    "stop": "kill-port 4200 9300"
  },
  "pre-commit": [
    "build",
    "scsslint",
    "tslint"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.11",
    "@angular/cdk": "^7.1.1",
    "@angular/common": "~9.1.11",
    "@angular/compiler": "~9.1.11",
    "@angular/core": "~9.1.11",
    "@angular/forms": "~9.1.11",
    "@angular/material": "^7.1.1",
    "@angular/platform-browser": "~9.1.11",
    "@angular/platform-browser-dynamic": "~9.1.11",
    "@angular/router": "~9.1.11",
    "@fullcalendar/core": "^5.0.0",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^8.6.0",
    "@ngrx/router-store": "^8.6.0",
    "@ngrx/store": "^8.6.0",
    "@ngrx/store-devtools": "^8.6.0",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^6.0.0",
    "bootstrap": "^4.4.1",
    "chart.js": "^2.9.3",
    "check-engines": "^1.5.0",
    "core-js": "^2.5.4",
    "font-awesome": "^4.7.0",
    "jest-preset-angular": "^8.2.1",
    "jquery": "^3.5.0",
    "kill-port": "^1.6.0",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.16.1",
    "primeicons": "^1.0.0",
    "primeng": "^9.0.0-rc.3",
    "quill": "^1.3.7",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.0",
    "@angular/cli": "~9.1.9",
    "@angular/compiler-cli": "~9.1.11",
    "@angular/language-service": "~9.1.11",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^26.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "^3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^26.1.0",
    "jest-html-reporter": "^3.1.3",
    "mock-responses": "2.2.7",
    "pre-commit": "^1.2.2",
    "protractor": "~5.4.0",
    "sass-lint": "^1.13.1",
    "ts-node": "~7.0.0",
    "tslint": "^5.11.0",
    "typescript": "~3.8.3"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/setupJest.ts"
  },
  "sasslintConfig": ".sass-lint.yml"
}
